
public class Loopjava {

	public static void printNumbers(int start,int end);{
	    while(start <= end) {
	    System.out.println("Count:"+start);
	    Thread.sleep(1000 * 1);
	    start++;
	    }
	    System.out.println("end...");
	    }
	    public static void main(String[] args) throws InterruptedException{
	    printNumbers(1,10);
	    }


	}

   
